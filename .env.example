# Docker Compose Environment Variables

# JWT Secret (IMPORTANT: Change this in production!)
# Generate a secure random string with: openssl rand -base64 64
JWT_SECRET=change-this-secret-in-production-use-a-long-random-string-at-least-64-characters

# PostgreSQL
POSTGRES_USER=panicless
POSTGRES_PASSWORD=panicless_dev
POSTGRES_DB=panicless_library

# API Base URL for frontend
VITE_API_BASE_URL=http://localhost:8080
